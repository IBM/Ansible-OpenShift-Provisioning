variant: openshift
version: 4.19.0
metadata:
  name: worker-luks-storage
  labels:
    machineconfiguration.openshift.io/role: worker
openshift:
  fips: true
  kernel_arguments:
    - rd.luks.key=/etc/luks/cex.key
boot_device:
  layout: s390x-virt
  luks:
    device: /dev/disk/by-partlabel/root
    cex:
      enabled: true
